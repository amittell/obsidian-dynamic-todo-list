{
  "name": "obsidian-dynamic-todo-list",
  "version": "1.0.0",
  "description": "A dynamic todo list plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "tsc --noEmit --skipLibCheck"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.25.0",
    "eslint": "^8.47.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "obsidian": "latest",
    "typescript": "^4.7.4",
    "typescript-eslint": "^8.35.0"
  }
}
